server          = false
datacenter      = "dc1"
node_name       = "{{ ansible_hostname }}"
data_dir        = "/var/lib/consul"
bind_addr       = "{{ ansible_enp0s8['ipv4']['address'] }}"
client_addr     = "127.0.0.1"
retry_join      = [ "{{ clusters['consul']|join("\",\"") }}" ]
log_level       = "DEBUG"
enable_syslog   = true